{
  "//": "@todo-symfony",
  "name": "xm_starter_symfony",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xmmedia/starter_symfony"
  },
  "license": "MIT",
  "//private": true,
  "scripts": {
    "dev": "./node_modules/.bin/encore dev",
    "dev-server": "./node_modules/.bin/encore dev-server --config webpack.dev-server.config.js --hot --port 9001 --public dev.example.com:443",
    "test": "karma start --single-run --browsers ChromeHeadless karma.config.js",
    "test-watch": "karma start --auto-watch --browsers ChromeHeadless karma.config.js",
    "test:e2e": "cypress open",
    "lint": "eslint --ext .js,.vue public/js",
    "lint:fix": "eslint --ext .js,.vue public/js --fix",
    "watch": "./node_modules/.bin/encore dev --watch",
    "build": "./node_modules/.bin/encore production",
    "stats": "./node_modules/.bin/encore production --profile --json > webpack-status.json"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@symfony/webpack-encore": "git://github.com/symfony/webpack-encore.git#master",
    "autoprefixer": "^9.2.1",
    "axios": "^0.18.0",
    "css-loader": "^1.0.0",
    "glob-all": "^3.1.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.9.4",
    "portal-vue": "^1.4.0",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.3.1",
    "sass-loader": "^7.1.0",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "tailwindcss": "^0.6.6",
    "vue": "^2.1",
    "vue-js-modal": "^1.3.13",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.0",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "devDependencies": {
    "@vue/eslint-config-prettier": "^3.0.5",
    "cypress": "^3.1.0",
    "eslint": "^5.7.0",
    "eslint-plugin-vue": "^4.7.1",
    "faker": "^4.1.0",
    "inject-loader": "^4.0.1",
    "moxios": "^0.4.0",
    "vue-test-utils": "^1.0.0-beta.6",
    "webpack-merge": "^4.1.0"
  }
}
